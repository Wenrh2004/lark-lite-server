{
    "swagger": "2.0",
    "info": {
        "description": "This is a sample server celler server.",
        "title": "Lark Lite User Server API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "API Support",
            "url": "http://www.swagger.io/support",
            "email": "support@swagger.io"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        },
        "version": "1.0.0"
    },
    "host": "localhost:8888",
    "basePath": "/api/v1",
    "paths": {
        "/api/v1/user/login": {
            "post": {
                "description": "用户账户登录认证",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "用户认证"
                ],
                "summary": "用户登录",
                "parameters": [
                    {
                        "description": "用户登录请求",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/github_com_Wenrh2004_lark-lite-server_common_api_v1.UserAuthRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "登录成功",
                        "schema": {
                            "$ref": "#/definitions/github_com_Wenrh2004_lark-lite-server_common_api_v1.UserAuthResponse"
                        }
                    },
                    "400": {
                        "description": "请求参数错误",
                        "schema": {
                            "$ref": "#/definitions/github_com_Wenrh2004_lark-lite-server_common_api_v1.ErrorResponse"
                        }
                    },
                    "401": {
                        "description": "认证失败",
                        "schema": {
                            "$ref": "#/definitions/github_com_Wenrh2004_lark-lite-server_common_api_v1.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "$ref": "#/definitions/github_com_Wenrh2004_lark-lite-server_common_api_v1.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/api/v1/user/refresh": {
            "post": {
                "description": "使用刷新令牌获取新的访问令牌",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "用户认证"
                ],
                "summary": "刷新访问令牌",
                "parameters": [
                    {
                        "type": "string",
                        "description": "刷新令牌",
                        "name": "UserRefreshToken",
                        "in": "header"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "刷新成功",
                        "schema": {
                            "$ref": "#/definitions/github_com_Wenrh2004_lark-lite-server_common_api_v1.RefreshResponse"
                        }
                    },
                    "401": {
                        "description": "认证失败或令牌过期",
                        "schema": {
                            "$ref": "#/definitions/github_com_Wenrh2004_lark-lite-server_common_api_v1.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "$ref": "#/definitions/github_com_Wenrh2004_lark-lite-server_common_api_v1.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/api/v1/user/register": {
            "post": {
                "description": "创建新用户账户",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "用户认证"
                ],
                "summary": "用户注册",
                "parameters": [
                    {
                        "description": "用户注册请求",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/github_com_Wenrh2004_lark-lite-server_common_api_v1.UserAuthRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "注册成功",
                        "schema": {
                            "$ref": "#/definitions/github_com_Wenrh2004_lark-lite-server_common_api_v1.UserAuthResponse"
                        }
                    },
                    "400": {
                        "description": "请求参数错误",
                        "schema": {
                            "$ref": "#/definitions/github_com_Wenrh2004_lark-lite-server_common_api_v1.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "$ref": "#/definitions/github_com_Wenrh2004_lark-lite-server_common_api_v1.ErrorResponse"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "github_com_Wenrh2004_lark-lite-server_common_api_v1.ErrorResponse": {
            "description": "API错误响应格式",
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer",
                    "example": 400
                },
                "message": {
                    "type": "string",
                    "example": "请求参数错误"
                }
            }
        },
        "github_com_Wenrh2004_lark-lite-server_common_api_v1.RefreshResponse": {
            "description": "刷新访问令牌响应",
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "#/definitions/github_com_Wenrh2004_lark-lite-server_common_api_v1.UserCertificateResponseBody"
                },
                "resp": {
                    "$ref": "#/definitions/github_com_Wenrh2004_lark-lite-server_common_api_v1.Response"
                }
            }
        },
        "github_com_Wenrh2004_lark-lite-server_common_api_v1.Response": {
            "description": "API通用响应格式",
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer",
                    "example": 0
                },
                "data": {},
                "message": {
                    "type": "string",
                    "example": "success"
                }
            }
        },
        "github_com_Wenrh2004_lark-lite-server_common_api_v1.UserAuthRequest": {
            "description": "用户登录/注册请求参数",
            "type": "object",
            "required": [
                "password",
                "username"
            ],
            "properties": {
                "password": {
                    "type": "string",
                    "example": "password123"
                },
                "username": {
                    "type": "string",
                    "example": "testuser"
                }
            }
        },
        "github_com_Wenrh2004_lark-lite-server_common_api_v1.UserAuthResponse": {
            "description": "用户登录/注册响应",
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "#/definitions/github_com_Wenrh2004_lark-lite-server_common_api_v1.UserAuthResponseBody"
                },
                "resp": {
                    "$ref": "#/definitions/github_com_Wenrh2004_lark-lite-server_common_api_v1.Response"
                }
            }
        },
        "github_com_Wenrh2004_lark-lite-server_common_api_v1.UserAuthResponseBody": {
            "description": "用户认证成功返回的用户信息",
            "type": "object",
            "properties": {
                "avatar_url": {
                    "type": "string",
                    "example": "https://example.com/avatar.jpg"
                },
                "certificate": {
                    "$ref": "#/definitions/github_com_Wenrh2004_lark-lite-server_common_api_v1.UserCertificateResponseBody"
                },
                "nickname": {
                    "type": "string",
                    "example": "测试用户"
                },
                "user_id": {
                    "type": "string",
                    "example": "123456789"
                },
                "username": {
                    "type": "string",
                    "example": "testuser"
                }
            }
        },
        "github_com_Wenrh2004_lark-lite-server_common_api_v1.UserCertificateResponseBody": {
            "description": "用户访问凭证信息",
            "type": "object",
            "properties": {
                "certificate": {
                    "type": "string",
                    "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
                },
                "expires_in": {
                    "type": "integer",
                    "example": 3600
                }
            }
        }
    },
    "securityDefinitions": {
        "Bearer": {
            "type": "apiKey",
            "name": "Authorization",
            "in": "header"
        }
    }
}